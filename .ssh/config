Host github.com-work
  HostName github.com
  User git
  Port 22
  IdentityFile ~/.ssh/rsa/work_rsa
  TCPKeepAlive yes
  IdentitiesOnly yes
  UseKeychain yes
	AddKeysToAgent yes
Host github.com-private
  HostName github.com
  User git
  Port 22
  IdentityFile ~/.ssh/rsa/private_rsa
  TCPKeepAlive yes
  IdentitiesOnly yes
  UseKeychain yes
	AddKeysToAgent yes

Host local
  HostName 192.168.0.9
  User shiomitomoaki
  Port 22
  IdentityFile ~/.ssh/rsa/local_rsa

