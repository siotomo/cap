# glomouth
Host *.app.*.glomouth *.batch.*.glomouth *.wp.*.glomouth job.kintone.manage.glomouth
  User ec2-user
  ProxyCommand ssh -CW %h:%p ec2-user@blue.deploy.glomouth
Match host *.glomouth user ec2-user
  IdentityFile ~/.ssh/glomouth.pem

Host blue.deploy.glomouth
  HostName deploy.blue.shikakara.jp
  Port 29003
  User deploy

#git
Host github.com-work
  HostName github.com
  User git
  Port 22
  IdentityFile ~/.ssh/rsa/work_rsa
  TCPKeepAlive yes
  IdentitiesOnly yes
  UseKeychain yes
	AddKeysToAgent yes
Host github.com-private
  HostName github.com
  User git
  Port 22
  IdentityFile ~/.ssh/rsa/private_rsa
  TCPKeepAlive yes
  IdentitiesOnly yes
  UseKeychain yes
	AddKeysToAgent yes

